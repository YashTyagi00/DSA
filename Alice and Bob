#include <iostream>
#include <stack>
using namespace std;

int main() {
    string inputString;
    getline(cin, inputString); 

    stack<char> charStack;

    for (char c : inputString) {
        charStack.push(c);
    }

    string reversedString = "";

    while (!charStack.empty()) {
        reversedString += charStack.top();
        charStack.pop();
    }

    cout << reversedString << endl;

    return 0;
}

